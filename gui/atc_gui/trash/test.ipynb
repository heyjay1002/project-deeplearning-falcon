   "source": [
    "import sys\n",
    "from PyQt6.QtWidgets import QApplication, QMainWindow\n",
    "\n",
    "app = QApplication(sys.argv)\n",
    "\n",
    "# Fusion 스타일 (보통 회색 타이틀바)\n",
    "app.setStyle('Fusion')\n",
    "\n",
    "window = QMainWindow()\n",
    "window.setWindowTitle(\"FALCON - Fusion Style\")\n",
    "window.resize(800, 600)\n",
    "window.show()\n",
    "\n",
    "sys.exit(app.exec())"
   ]
